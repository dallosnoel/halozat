enable
configure terminal
hostname sw1

configure terminal
username 1234 privilege 15 secret 1234
enable password 1234
end

configure terminal
ip domain-name winserver.winserver.lan
cry key gen rsa
1024

line con 0
password 1234
login local
end

configure terminal
enable secret 1234
end

configure terminal
line vty 0 15
password 1234
trans input ssh
login local
end

configure terminal
ip ftp username 1234
ip ftp password 1234

int vlan 1
ip address 192.168.1.2 255.255.255.0
no sh
ip default-gate 192.168.1.1
end

configure terminal
service password-encryption
end

configure terminal
banner motd 'Belepes idegeneknek tilos'
end


copy r st
